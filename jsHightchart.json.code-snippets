      "                fillColor: 'white'",
      "            }",
      "        }, {",
      "            type: 'pie',",
      "            name: '总消费',",
      "            data: [{",
      "                name: 'Jane',",
      "                y: 13,",
      "                color: Highcharts.getOptions().colors[0] // Jane 的颜色",
      "            }, {",
      "                name: 'John',",
      "                y: 23,",
      "                color: Highcharts.getOptions().colors[1] // John 的颜色",
      "            }, {",
      "                name: 'Joe',",
      "                y: 19,",
      "                color: Highcharts.getOptions().colors[2] // Joe 的颜色",
      "            }],",
      "            center: [100, 80],",
      "            size: 100,",
      "            showInLegend: false,",
      "            dataLabels: {",
      "                enabled: false",
      "            }",
      "      }",
      "   ];     ",
      "      ",
      "   var json = {};   ",
      "   json.title = title;   ",
      "   json.xAxis = xAxis;",
      "   json.labels = labels;  ",
      "   json.series = series;",
      "   $('#container').highcharts(json);  ",
      "});",
      "</script>",
      "</body>",
      "</html>"
