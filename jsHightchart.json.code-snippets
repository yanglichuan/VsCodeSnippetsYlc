      "      pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'",
      "   };",
      "   var plotOptions = {",
      "      pie: {",
      "         dataLabels: {",
      "            enabled: true,",
      "            distance: -50,",
      "            style: {",
      "               fontWeight: 'bold',",
      "               color: 'white',",
      "               textShadow: '0px 1px 2px black'",
      "            }",
      "         },",
      "         startAngle: -90,",
      "         endAngle: 90,",
      "         center: ['50%', '75%']",
      "      }",
      "   };",
      "   var series= [{",
      "      type: 'pie',",
      "      name: 'Browser share',",
      "      innerSize: '50%',",
